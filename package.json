{
  "name": "gato",
  "productName": "gato",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Cesar Varela",
    "email": "mail@cesarvarela.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/logo"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "gato"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/windows/main/index.html",
                  "js": "./src/windows/main/renderer.tsx",
                  "name": "main",
                  "preload": {
                    "js": "./src/windows/main/preload.ts"
                  }
                },
                {
                  "name": "persona_shared",
                  "js": "./src/windows/persona_shared/dummy.ts",
                  "preload": {
                    "js": "./src/windows/persona_shared/preload.ts"
                  }
                },
                {
                  "html": "./src/windows/persona_shared/index.html",
                  "js": "./src/windows/search/renderer.tsx",
                  "name": "search"
                },
                {
                  "html": "./src/windows/persona_shared/index.html",
                  "js": "./src/windows/home/renderer.tsx",
                  "name": "home"
                },
                {
                  "html": "./src/windows/persona_shared/index.html",
                  "js": "./src/windows/read/renderer.tsx",
                  "name": "read"
                },
                {
                  "html": "./src/windows/persona_shared/index.html",
                  "js": "./src/windows/youtube/renderer.tsx",
                  "name": "youtube"
                },
                {
                  "html": "./src/windows/persona_shared/index.html",
                  "js": "./src/windows/error/renderer.tsx",
                  "name": "error"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "@types/mousetrap": "^1.6.8",
    "@types/react-dom": "^17.0.11",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "autoprefixer": "^10.4.0",
    "css-loader": "^6.0.0",
    "electron": "16.0.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.0.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@postlight/mercury-parser": "^2.2.1",
    "@types/lodash": "^4.14.177",
    "@u-wave/react-youtube": "^0.7.2",
    "electron-context-menu": "^3.1.1",
    "electron-dl": "^3.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "googleapis": "^92.0.0",
    "got": "^12.0.0",
    "jsdom": "^19.0.0",
    "match-url-wildcard": "^0.0.4",
    "mousetrap": "^1.6.5",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss-classnames": "^2.2.6",
    "use-query-params": "^1.2.3",
    "validator": "^13.7.0"
  }
}